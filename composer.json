{
    "name": "debril/rss-atom-bundle",
    "type": "symfony-bundle",
    "description": "RSS and Atom support for Symfony 2",
    "keywords": ["rss", "atom"],
    "license": "LGPL",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/alexdebril/rss-atom-bundle.git"
        }
    ],
    "require": {
        "php": ">=5.4",
        "symfony/config": "~2.6",
        "debril/feed-io": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.4",
        "doctrine/common": "~2.3",
        "symfony/framework-bundle": "~2.6",
        "symfony/validator": "~2.6",
        "symfony/finder": "~2.6",
        "symfony/form": "~2.6",
        "symfony/browser-kit": "~2.6"
    },
    "autoload": {
        "psr-0": {
            "Debril\\RssAtomBundle": ""
        }
    },
    "target-dir": "Debril/RssAtomBundle",
    "config": {
        "bin-dir": "bin"
    }
}
